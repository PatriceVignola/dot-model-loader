{
  "name": "dot-model-loader",
  "version": "0.0.1",
  "description": "A lightweight JavaScript loader for .model 3D files",
  "main": "./lib/index.js",
  "scripts": {
    "prettier": "prettier es6 --write src/**/*.js",
    "test": "jest --coverage",
    "lint": "eslint .",
    "flow": "flow",
    "validate": "run-p lint flow test --silent",
    "build-clean": "rimraf lib dist coverage",
    "build-lib": "babel src -d lib --ignore '**/*.test.js' && flow gen-flow-files src/ --out-dir lib/",
    "build-dist": "webpack",
    "build": "yarn build-clean && yarn build-lib && yarn build-dist",
    "release": "yarn build && yarn validate && yarn release-it"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn validate"
    }
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PatriceVignola/dot-model-loader.git"
  },
  "keywords": [
    "wwdc2010",
    "glessentials",
    "3d",
    "model",
    "loader",
    "opengl",
    "webgl"
  ],
  "author": "Patrice Vignola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PatriceVignola/dot-model-loader/issues"
  },
  "homepage": "https://github.com/PatriceVignola/dot-model-loader#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.72.0",
    "flow-typed": "^2.4.0",
    "jest": "^22.4.3",
    "npm-run-all": "^4.1.3",
    "prettier": "1.12.1",
    "release-it": "^7.4.5",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "file-stream-reader": "^1.0.1",
    "husky": "^1.0.0-rc.6"
  }
}
